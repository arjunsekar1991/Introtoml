library(rpart.plot)
tree.train = rpart(Class~., data=trainingData)
fancyRpartPlot(tree.train)
tree.predict <- predict(tree.train, testData, type="class")
install.packages('caret', dependencies = TRUE)
library(caret)
confusionMatrix(tree.predict, testData$Class)
